        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FechaRegistro)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="IdProducto" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index"class="btn btn-secondary btn-sm">Volver atrás</a>
    </form>
</div>
</section>